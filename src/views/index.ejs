<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./partials/_head") %>
  </head>
  <body>
    <header><%- include("./partials/_header") %></header>

    <main>
      <div class="jumbotron">
        <h1>This is great</h1>
        <p>Welcome to templating using EJS</p>

        <h2>Single Variable</h2>
        <p><%= tagline %></p>

        <ul>
          <% mascots.forEach(function(mascot) { %>
          <li>
            <strong>Name:</strong> <%= mascot.name %>, <strong>Age:</strong> <%=
            mascot.age %>, <strong>Email:</strong> <%= mascot.email %>
          </li>
          <% }); %>
        </ul>
      </div>
    </main>

    <footer><%- include("./partials/_footer") %></footer>
  </body>
</html>
